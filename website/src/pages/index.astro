---
import Layout from '../layouts/Layout.astro';
import ImageList from '../components/ImageList.astro';

const title = '每天一句古诗词，生成 AI 图片';

---

<Layout title={title}>
  <div class="flex justify-center mt-12 text-xl px-5">
    {title}
  </div>

 <Transition appear name="fade" mode="out-in">
	<ImageList />
</Transition>
</Layout>
